@model IEnumerable<UsersManager.Models.Photo>

@Scripts.Render("~/bundles/ratings")

@if (Model.Count() == 0)
{
    <h4 class="text-center text-warning">Aucune photo! Ajuster vos filtres...</h4>
}
else
{
    foreach (var photo in Model)
    {
        <div class="photoContainer">
            @Helper.AvatarUser(photo.User)
            @Helper.Photo(photo)
        </div>
    }
}